{
  "name": "le5le-ng-start",
  "version": "0.0.1",
  "description": "Angular start for le5le.",
  "main": "index.ts",
  "scripts": {
    "clean": "npm cache clean && npm run rimraf -- dist ",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "dev:aot": "webpack-dev-server --config webpack.aot.js --open --progress --profile --watch --content-base src/",
    "start": "webpack-dev-server --config webpack.config.js --open --progress --profile --watch --content-base src/",
    "build": "npm run clean && webpack --config webpack.aot.ts --colors --progress",
    "test": "npm run lint && karma start"
  },
  "keywords": [
    "angular",
    "le5le"
  ],
  "author": "alsmile123@qq.com",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.0.0",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/platform-server": "~4.0.0",
    "@angular/router": "~4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "~5.1.0",
    "zone.js": "~0.8.4",
    "crypto-js": "^3.1.9-1",
    "le5le-ui": "~0.1.3",
    "le5le-components": "~0.1.1",
    "le5le-store": "~0.0.5",
    "normalize.css": "~4.1.1",
    "file-saver": "*"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~4.0.0",
    "@ngtools/webpack": "^1.3.0",
    "@types/crypto-js": "^3.1.33",
    "@types/node": "^7.0.10",
    "@types/source-map": "^0.5.0",
    "@types/webpack": "^2.0.0",

    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "~3.1.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "~2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "compression-webpack-plugin": "^0.3.2",
    "autoprefixer": "^6.1.1",
    "postcss": "^5.0.12",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "cssnano": "^3.10.0",
    "raw-loader": "0.5.1",
    "rimraf": "~2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-node": "^2.0.0",
    "tslint": "~4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.1.5",
    "url-loader": "^0.5.7",
    "file-loader": "~0.10.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "~3.0.0"
  }
}
